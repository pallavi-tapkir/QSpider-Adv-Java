<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <context-param>
  	<param-name>cgst</param-name>
  	<param-value>0.77</param-value>
  </context-param>
  
  <servlet>
  	<servlet-name>xyz</servlet-name>
  	<servlet-class>product_servlet.ProductController</servlet-class>
  	
  	<init-param>
  		<param-name>MAH</param-name>
  		<param-value>0.18</param-value>
  	</init-param>
  	
  	<init-param>
  		<param-name>KAR</param-name>
  		<param-value>0.14</param-value>
  	</init-param>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>xyz</servlet-name>
  	<url-pattern>/product</url-pattern>
  </servlet-mapping>
</web-app>
